<a href=<%= "/strain_info/"+encodeURI(strain.strain_name) %>>
    <div>
        <div id="strainInfo">
            <img id="myStrainPic" src="<%= strain.image_url %>">
            <p><%= strain.strain_name %></p>
            <form action="/my_strain_delete/<%= results.id %>" method="POST">
                <button class="btn btn-success">Delete From My Trees</button>
            </form>
        </div>
    </div>
</a>